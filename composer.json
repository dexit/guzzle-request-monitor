{
  "name": "intelligents-nl/guzzle-request-monitor",
  "autoload": {
    "psr-4": {
      "IntelligentsNL\\GuzzleRequestMonitor\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "require": {
    "php": ">=8.1",
    "laravel/framework": "^10.0",
    "illuminate/support": "^10.0",
    "guzzlehttp/guzzle": "^7.2"
  },
  "extra": {
    "laravel": {
      "providers": [
        "IntelligentsNL\\GuzzleRequestMonitor\\Providers\\GuzzleRequestMonitorServiceProvider"
      ]
    }
  },
  "suggest": {
    "pagevamp/laravel-cloudwatch-logs": "Required to use this package with Cloudwatch"
  }
}
